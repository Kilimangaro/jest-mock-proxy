{
  "root": true,
  "env": {
    "jest": true
  },
  "plugins": ["jest"],
  "extends": [
    "plugin:jest/recommended",
    "xo",
    "xo-typescript",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/unified-signatures": "off"
  }
}
